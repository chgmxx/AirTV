<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.goby.airtv"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="5" />
    <permission android:name="android.permission.ACCESS_NETWORK_STATE"></permission>
    <permission android:name="android.permission.INTERNET"></permission>
    <permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE"></permission>
    <permission android:name="android.permission.ACCESS_WIFI_STATE"></permission>
    <permission android:name="android.permission.CHANGE_NETWORK_STATE"></permission>
    <permission android:name="android.permission.CHANGE_WIFI_STATE"></permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>